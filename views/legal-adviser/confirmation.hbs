{{#with search}}

<div class="persist-info">
  <div class="persist-info-content">
    <span class="persist-info-reference">{{personal-details.urn}}</span>
    <span class="persist-info-title"><b>{{personal-details.firstname}} {{uppercase personal-details.lastname}}</b></span>
  </div>
  <ul class="persist-info-links">
    <li><a href="/legal-adviser/end-session/">End session</a></li>
  </ul>
</div>

<div class="grid-row">

  <div class="column-two-thirds">

    <div class="notification notification-confirmation" role="alert">
      <i class="icon icon-tick-white" role="presentation" aria-hidden="true"></i>
      <div class="notification-message">
        <h1 class="bold-medium">Decision submitted</h1>
        <p>Case 12345667890 complete</p>
      </div>
    </div>

    <p><a class="button" href="/legal-adviser/case-details/1/" role="button">Continue to next case</a></p>

  </div>

</div>

{{/with}}
